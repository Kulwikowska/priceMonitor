<% include partials/header %>


    <h1 id="login">Register</h1>
    <div class="row">
        <div style="width: 30%; margin: 25px auto;">
            <form action="/register" method="post">
                <div class="form-group">
                    <input class="form-control" type="text" name="username" placeholder="username">
                </div>
                <div class="form-group">
                    <input class="form-control" type="password" name="password" placeholder="password">
                </div>
                <div class="form-group">
                    <input type="text" value="B" name="B" disabled>
                </div>
                <div class="form-group">
                    <input type="hidden" value="C" name="C">
                </div>
                <div class="form-group">
                    <input type="text" value="D" name="D" hidden>
                </div>
                <div class="form-group">
                    <input type="text" value="E" name="E" style="display:none">
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-light btn-block">Sign up!</button>
                </div>
            </form>
            <a href="/">Go Back</a>
        </div>
    </div>

    <% include partials/footer %>